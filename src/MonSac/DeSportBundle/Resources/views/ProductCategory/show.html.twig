{% extends 'MonSacDeSportBundle::admin_layout.html.twig' %}

{% block content -%}
    <h1>ProductCategory</h1>

    <table class="record_properties">
        <tbody>

            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('productcategory') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('productcategory_edit', { 'slug': entity.slug }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
