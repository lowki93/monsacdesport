<?php

namespace MonSac\DeSportBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="fos_user")
 */
class User extends BaseUser
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="text", name="first_name", length=255)
     */
    protected $firstName;

    /**
     * @ORM\Column(type="text", name="last_name", length=255)
     */
    protected $lastName;

    /**
     * @ORM\Column(type="text", name="address", length=255)
     */
    protected $address;

    /**
     * @ORM\Column(type="text", name="zipcode", length=10)
     */
    protected $zipcode;

    /**
     * @ORM\Column(type="text", name="city", length=255)
     */
    protected $city;

    /**
     * @ORM\Column(type="text", name="phone", length=15)
     */
    protected $phone;

    /**
     * @ORM\Column(type="textarea", name="complementary_info", length=255)
     */
    protected $complementary_info;

    /**
     * @ORM\OneToMany(targetEntity="Commande", mappedBy="owner")
     */
    protected $commandes;


    public function __construct()
    {
        parent::__construct();
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
